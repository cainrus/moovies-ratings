{
  "content_scripts": [
    {
      "css": [
        "css/app.css"
      ],
      "js": [
        "js/app.js"
      ],
      "matches": [
        "https://rutracker.org/*",
        "https://rutracker.net/*",
        "https://rutracker.nl/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "An easy to use rutracker movie rating extension will add extra column with rating",
  "icons": {
    "128": "images/128x128.jpg",
    "48": "images/48x48.jpg"
  },
  "manifest_version": 3,
  "name": "Rutracker MOOvie rating",
  "version": "1.0.3"
}
